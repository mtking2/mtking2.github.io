{
  "name": "pug-site",
  "version": "1.0.0",
  "description": "",
  "main": "",
  "scripts": {
    "compile": "npm run precompile && node compile.js 2>&1 | tee log/compile.log",
    "precompile": "rm -rf dist && mkdir -p dist && ./node_modules/.bin/browserify ./index.js > ./dist/index.js && ./node_modules/.bin/uglifyjs ./dist/index.js -o ./dist/index.js",
    "watch": "nodemon -e js,sass,pug -i dist -x \"npm run compile && http-server -c-1\"",
    "start": "npm run watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mtking2/mtking2.github.io.git"
