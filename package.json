  "devDependencies": {
    "async": "^1.4.2",
    "axios": "^0.18.1",
    "browserify": "^13.0.0",
    "dancing-dots": "0.0.2",
    "delaunay-fast": "^1.0.1",
    "dotenv": "^1.2.0",
    "front-matter": "^3.1.0",
    "http-server": "^0.12.1",
    "lodash": "^4.17.15",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
